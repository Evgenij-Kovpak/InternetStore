@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}

<h1>Пользователи</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Имя пользователя
            </th>
            <th>
                Почта
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.UserName
            </td>
            <td>
                @item.Email
            </td>

        </tr>
}
    </tbody>
</table>
